{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd marketing-site && npm run build",
  "outputDirectory": "marketing-site/.next",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 